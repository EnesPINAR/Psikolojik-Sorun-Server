

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin</title>
</head>
<body>
    <nav><a href="/admin/psychology_fields">Psikolog Alanları</a></nav>
    <h4>Admin isim:<%=admin.firstName%></h4>
    <h4>Admin soyisim:<%=admin.lastName%></h4>
    <h4>Admin email:<%=admin.email%></h4>
    <hr style="width: 100%; height: 2px; background-color: black;">
    <h2>Onaylanmayı Bekleyen Psikologlar</h2>
    <% psychologist.forEach(function(psychologist){ %>
        <ul>
            <li style="display: flex;"><%= psychologist.firstName %> <%= psychologist.lastName %> <a style="margin-left: 5px;" target="_blank" href="/admin/dashboard/inspect/<%=psychologist._id%>">İncele</a><form method="POST" action="/admin/approve"><input type="hidden" name="approve" value="<%= psychologist._id%>"><button style="margin-left: 5px;" type="submit">Onayla</button></form></li>
        </ul>
      <% }); %>
</body>

</html>